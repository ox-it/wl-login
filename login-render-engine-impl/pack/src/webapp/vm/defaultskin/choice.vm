<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
	"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<link href="${pageSkinRepo}/tool_base.css" type="text/css" rel="stylesheet" media="all" />
		<link href="${pageSkinRepo}/${pageSkin}/tool.css" type="text/css" rel="stylesheet" media="all" />
		<link href="${pageSkinRepo}/${pageSkin}/mobile.css" type="text/css" rel="stylesheet" media="only screen and (max-device-width: 600px)" />
		<link href="${pageSkinRepo}/${pageSkin}/mobile.css" type="text/css" rel="stylesheet" media="handheld" />
		<meta http-equiv="Content-Style-Type" content="text/css" />
		<title>${uiService} : ${loginRequired}</title>
		<script type="text/javascript" language="JavaScript" src="${pageScriptPath}headscripts.js"></script>
		<meta name="viewport" content="width=320, user-scalable=no"/>
	</head>
	<body onload="parent.updCourier(doubleDeep, ignoreCourier);">
		<!-- choice.vm -->
		<table class="login" cellpadding="0" cellspacing="0" border="0" summary="layout">
		<tr>
			<th colspan="2">${uiService} : ${loginRequired}</th>
		</tr>
		<tr>
			<td class="logo"></td>
			<td class="form">
				#if (${notify})
					<div class="alertMessage">${notify}</div>
				#end
					<table border="0" class="loginform" summary="layout" style="text-align: center">
					<tr>
						<td>
						<div class="instruction">
						${loginSelect}
						</div>
						</td>
					</tr>
					<tr>
						<td><a href="${loginURL}">${loginText}</a></td>
					</tr>
					<tr>
						<td><a href="${xloginURL}">${xloginText}</a></td>
					</tr>
					</table>
			</td>
		</tr>
		</table>
		<!-- end choice.vm -->
	</body>
</html>
